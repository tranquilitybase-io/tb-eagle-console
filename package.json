{
  "name": "tb-eagle-console-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -- --proxy-config ./proxy.conf.js --host 0.0.0.0 --disable-host-check true",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
