    "@ngrx/store-devtools": "~8.6.0",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~8.9.5",
    "codelyzer": "~5.2.2",
    "husky": "~4.2.5",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "prettier": "~1.19.1",
    "pretty-quick": "~2.0.1",
    "protractor": "^7.0.0",
    "tslint": "^6.1.2"
